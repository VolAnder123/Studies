unit Unit2;

interface
uses unit1;
procedure udal(var a : Pmas; n : integer; var n1 : integer);
implementation

procedure udal(var a : Pmas; n : integer; var n1 : integer);
var i : integer;
begin
for i:=1 to n do
if a[i] mod 2=1 then begin
                     inc(n1);
                     a[n1]:=a[i];
                     end;
end;
end.
 